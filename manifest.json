{
  "manifest_version": 2,
  "name": "__MSG_ExtensionName__",
  "version": "<%= package.version %>",
  "description": "__MSG_ExtensionDescription__",
  "homepage_url": "<%= package.homepage %>",
  "default_locale": "en",
  "icons": {
    "16": "logo/logo-16.png",
    "32": "logo/logo-32.png",
    "48": "logo/logo-48.png",
    "64": "logo/logo-64.png",
    "96": "logo/logo-96.png",
    "128": "logo/logo-128.png"
  },
  "permissions": [
    "*://*.youtube.com/*",
    "*://*.youtube-nocookie.com/",
	"storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://*.youtube.com/*",
        "*://*.youtube-nocookie.com/"
      ],
      "js": [
        "vendor/browser-polyfill.min.js",
        "scripts/content.min.js"
      ]
    }
  ],
  "options_ui": {
	"page": "options.html"
  },
  "applications": {
	  "gecko": {
		  "id": "rthautYouTubePopoutPlayer@rthautYouTubePopoutPlayer.com"
	  }
  }
}
